<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="rootPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox spacing="10.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Label text="%ui.main.generator" />
                        <VBox spacing="6.0" VBox.vgrow="ALWAYS">
                           <children>
                              <CheckBox fx:id="chkMac" mnemonicParsing="false" onAction="#chkSelectorOnAction" text="%ui.main.generator.mac" />
                              <CheckBox fx:id="chkHost" mnemonicParsing="false" onAction="#chkSelectorOnAction" text="%ui.main.generator.host" />
                              <CheckBox fx:id="chkIp" mnemonicParsing="false" onAction="#chkSelectorOnAction" text="%ui.main.generator.ip" />
                              <CheckBox fx:id="chkOs" mnemonicParsing="false" onAction="#chkSelectorOnAction" text="%ui.main.generator.os" />
                              <CheckBox disable="true" mnemonicParsing="false" text="%ui.main.generator.notsupported" />
                              <CheckBox disable="true" mnemonicParsing="false" text="%ui.main.generator.notsupported" />
                              <CheckBox disable="true" mnemonicParsing="false" text="%ui.main.generator.notsupported" />
                              <CheckBox disable="true" mnemonicParsing="false" text="%ui.main.generator.notsupported" />
                           </children>
                        </VBox>
                     </children>
                  </VBox>
                  <VBox HBox.hgrow="SOMETIMES">
                     <children>
                        <Label text="%ui.main.id" />
                        <TextArea fx:id="txtId" editable="false" wrapText="true" />
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <Button fx:id="btnClose" mnemonicParsing="false" onAction="#btnCloseOnAction" text="%ui.main.btn.close" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
